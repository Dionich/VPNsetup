<Window x:Class="KeepSolid.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:KeepSolid"
        mc:Ignorable="d"
        Title="MainWindow" Height="400" Width="480"
        MaxHeight="400" MaxWidth="480"
        MinHeight="400" MinWidth="480">
    <Grid>



        <Rectangle x:Name="rectangle1" Width="501" Height="260"
                   HorizontalAlignment="Center" VerticalAlignment="Bottom" Margin="0 0 0 50"
                   Fill="#FFF3F3F3" Stroke="Black" StrokeThickness="0.3"/>

        <Rectangle x:Name="rectangle2" Width="501" Height="50"
                   HorizontalAlignment="Center" VerticalAlignment="Bottom"
                   Fill="#FFF3F3F3" Stroke="Black" StrokeThickness="0.3"/>




        <Image x:Name="Logo" Source="Resourses/logo.png" Width="50" Height="50"
               VerticalAlignment="Top" HorizontalAlignment="Right" Margin="10, 0"/>



    </Grid>
</Window>
